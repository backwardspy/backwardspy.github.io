{
  "name": "website",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "fmt": "prettier --write .",
    "lint": "eslint .",
    "preview": "astro preview"
  },
  "dependencies": {
    "@astrojs/check": "0.9.5",
    "@astrojs/mdx": "4.3.8",
    "@iconify-json/ph": "1.2.2",
    "astro": "5.15.1",
    "astro-icon": "1.1.5",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "sass": "1.93.2",
    "typescript-eslint": "^8.46.2"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
